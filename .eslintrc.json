{
  "extends": ["next/core-web-vitals", "plugin:react/recommended", "prettier"],
  "rules": {
    "no-var": "error",
    "no-unused-vars": "warn",
    "no-empty": "error",
    "no-empty-function": "warn",
    "dot-notation": "error",
    "react/react-in-jsx-scope": "off",
    "semi-style": "warn",
    "react/prop-types": "off"
  }
}
